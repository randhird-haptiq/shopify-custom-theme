   Note: "@import" rules arenâ€™t supported. */
@import url("https://fonts.googleapis.com/css2?family=Patua+One&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Assistant:wght@200..800&display=swap");

html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  display: block;
}
body {
  line-height: 1;
}
ol,
ul {
  list-style: none;
}
blockquote,
q {
  quotes: none;
}
blockquote:before,
blockquote:after,
q:before,
q:after {
  content: "";
  content: none;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}

// General Styling
button {
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: none;
  appearance: none;
  box-shadow: none;
  font: inherit;
  outline: none;
}
a {
  text-decoration: none;
  color: black;
}
::selection {
  background-color: black;
}
button,
input,
option,
textarea {
  margin: 0;
  padding: 0;
  background-color: transparent;
  border: none;
  appearance: none;
  box-shadow: none;
  outline: none;
}

// Start Variables
@font-face {
  font-family: "HelveticaNeueLTProBold";
  src: url('{{"HelveticaNeueLTPro-Bd.woff2" | asset_url}}') format("woff2");
}

@mixin phone {
  @media (max-width: 599px) {
    @content;
  }
}
@mixin tablet {
  @media (max-width: 900px) {
    @content;
  }
}
@mixin desktop {
  @media (max-width: 1200px) {
    @content;
  }
}

// Start Presets
.container,
.footer,
.header {
  padding: 0px 24px;
}

body {
  font-weight: 700;
  font-size: 16px;
  line-height: 1.25;
  font-family: "HelveticaNeueLTProBold", Arial, Helvetica, sans-serif;
}

.flex {
  display: flex;
}

.space-between {
  justify-content: space-between;
}
.column {
  flex-direction: column;
}
.half {
  width: 50%;
}
.section {
  padding: 25px;
}
img,
picture,
svg {
  max-width: 100%;
  vertical-align: middle;
}
.center {
  display: flex;
  justify-content: center;
  align-items: center;
}

// Start header
.header {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  background-color: white;
  z-index: 10;

  @include phone {
    position: static;
  }
}
.header-inner {
  width: 100%;
  height: 72px;
  align-items: center;
  border-bottom: 1px solid;

  @include phone {
    display: none;
  }
}
.header-title {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
}

// Start collection
.collection-list {
  display: flex;
  flex-wrap: wrap;
  gap: 25px; /* Space between the cards */
  justify-content: flex-start; /* Align cards to the start */
}

// Start Product
.product-card {
  // width: 30%; /* Each card takes 30% of the container width */
  width: calc((100% - 50px) / 3); /* Adjust for 3 cards per row */
  margin-bottom: 32px;
  display: flex;
  overflow: hidden;
  flex-direction: column;
  box-sizing: border-box; /* Ensure padding and border are included in the width */
  background-color: #fafafa;
}

@include tablet {
  .product-card {
    width: 48%; /* 2 cards per row on tablets */
  }
}

@include phone {
  .product-card {
    width: 100%; /* 1 card per row on phones */
  }
}
.product-image-wapper {
  padding-top: 120%;
  display: block;
  overflow: hidden;
  position: relative;
  margin-bottom: 28px;
}

// Start Produuct template
.swiper-container-product-page {
  overflow: hidden;
  width: 100%;
  height: 100%;
}

.product-template {
  padding-top: 25px;
  display: grid;
  grid-template-columns: calc((100% - 25px) / 2) calc((100% - 25px) / 2);
  justify-content: space-between;
  margin-bottom: 32px;

  @include phone {
    grid-template-columns: 100%;
  }
}

.swiper-button-next::before {
  background: url('{{"curson-arrow-right.svg" | asset_url}}') 24px center
    no-repeat;
  content: "";
  opacity: 0.25;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  transition: opacity 0.15s;
  background-position: calc(100% - 30px);
}

.swiper-button-prev::before {
  background: url('{{"curson-arrow-left.svg" | asset_url}}') 24px center
    no-repeat;
  content: "";
  opacity: 0.25;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  transition: opacity 0.15s;
  background-position: calc(100% - 30px);
}

.product-template {
  padding-top: 24px;
  display: grid;
  grid-template-columns: calc((100% - 25px) / 2) calc((100% - 25px) / 2);
}
.product-description-container {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.product-description {
  width: 50%;
  margin-bottom: 30px;

  @include phone {
    width: 100%;
  }
}
.product-title {
  margin-bottom: 100px;
  font-size: 24px;
  text-transform: uppercase;
}
.product-option-row {
  position: relative;
  user-select: none;
}

.product-option-label,
.add-to-cart-button {
  padding: 25px;
  position: relative;
  border: solid;
  cursor: pointer;
  border-color: black;
  border-width: 1px 1px 0;
  border-right-color: transparent;
  border-left-color: transparent;
  background-color: white;
  font-size: 16px;
  // width: 100%;
}
.add-to-cart-button {
  width: 100%;
  text-transform: uppercase;

  &:hover {
    background-color: black;
    color: white;
  }
}

.product-option-label:hover {
  background-color: #ededed;
}

.product-option-list {
  position: absolute;
  bottom: 70px;
  right: 0;
  left: 0;
  background-color: white;
  border: 1px solid black;
}

.product-option-item {
  padding: 25px;
  cursor: pointer;
  &:nth-child(+ 2) {
    box-shadow: inset 0 1px #9a9a9a;
  }

  &:hover {
    background-color: #ededed;
  }
}

// Start hero section
.hero-image {
  @include phone {
    height: 270px;

    img {
      height: 100%;
      object-fit: cover;
    }
  }
}

// Image with text section
.image-with-text {
  display: grid;
  grid-template-columns: 1fr 1fr;

  @include phone {
    grid-template-columns: 1fr;
  }
}
.image-with-text.reverse {
  direction: rtl;
}

.image-with-text-description-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 25px;
  direction: rtl;
}
.image-with-text-body {
  // width: 75%;
  margin: 0 auto;
  padding-top: 50px;
}

// start logo list
.logo-container {
  display: flex;
  justify-content: space-between;
}
.logo-link {
  max-width: 25%;
  img {
    width: 100%;
  }
}

// Start Mini cart
.mini-cart.hidden {
  display: none;
}

.miniCart.hidden {
  transform: translate(100%);
}

.miniCart.hidden::before {
  opacity: 0;
}

.mini-cart {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  height: 100vh;
  width: 100%;
  z-index: 99;
  max-width: 350px;
  background-color: white;
  padding: 72px 25px 25px 25px;
  transition-property: transform;

  &::before {
    content: "";
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    box-shadow: 0 4px 48px 36px rgba(0, 0, 0, 0.25);
    transition-property: opacity;
    z-index: -1;
  }
}

.mini-cart,
.mini-cart::before {
  transition-duration: 1s;
  transition-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
}

.cart-form {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.cart-button {
  cursor: pointer;
  position: relative;
  transition: 0.15s;
  z-index: 999;
}

.mini-cart-image-wrapper {
  width: 140px;
  margin: 25px;
  img {
    width: 100%;
  }
}

.mini-cart-rows {
  border-top: 1px solid black;
  border-bottom: 1px solid black;
  height: 100%;
  overflow: auto;
}

.mini-cart-tem {
  display: flex;
  padding: 25px 0;
}

.mini-cart-item-right {
  flex: 1;
}

.mini-cart-quantity {
  height: 25px;
  margin-top: 25px;
  position: relative;
  border: 1px solid;
  border-radius: 13px;

  input {
    width: 100%;
    height: 100%;
    text-align: center;
    line-height: 25px;
  }

  .mini-cart-quantity-button {
    width: 25px;
    position: absolute;
    top: -4px;
    bottom: 0;
    left: 0;
  }

  .mini-cart-quantity-button {
    left: auto;
    right: 0;
  }
  .button1 {
    left: 0;
  }
}

